Visualization Manager:
  Displays:
    - Name: Global Options
      Class: rviz/GlobalOptions
      Fixed Frame: base_link

    - Name: Robot Model
      Class: rviz/RobotModel
      Enabled: true

    - Name: LaserScan
      Class: rviz/LaserScan
      Topic: /scan
      Enabled: true
      Size: 0.05
      Alpha: 1.0
      Color: 255; 25; 25

    - Name: TF
      Class: rviz/TF
      Enabled: true
